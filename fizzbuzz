var count, replace;
count = function(begin, end){
	var output = "";
	for (i = begin; i <= end; i++){
		output += replace(i);
	}
	return output;
};

replace = function(num){
	if (num % 3 == 0 && num % 5 != 0)
		return "fizz ";
	else if (num % 3 != 0 && num % 5 == 0)
		return "buzz ";
	else if (num % 3 == 0 && num % 5 == 0)
		return "fizzbuzz ";
	else
		return (num + " ");
};

module.exports = {
	count: count,
	replace: replace
};